<?xml version="1.0" encoding="UTF-8"?>
<evaml
  name="teste"
  xsi:noNamespaceSchemaLocation="xml_schema/robot-schema.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <settings>
    <voice tone="en-US_EmmaExpressive" />
    <lightEffects mode="ON" />
    <audioEffects mode="ON" />
  </settings>
  <script>
    <light id="LOOP" state="ON" color="WHITE"/>
    <light state="OFF" color="WHITE"/>
    <random var="ALEATORIO" min="1" max="10" />
    <goto target="LOOP" />
  </script>
</evaml>
