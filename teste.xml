<?xml version="1.0" encoding="UTF-8"?>
<evaml
  name="teste"
  xsi:noNamespaceSchemaLocation="xml_schema/evaml_schema.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <settings>
    <voice tone="en-US_EmmaExpressive" />
    <lightEffects mode="ON" />
    <audioEffects mode="ON" />
  </settings>
  <script>
    <light id="LOOP1" state="OFF" />
    <wait duration="2000" />
    <switch var="7">
      <case op="exact" value="1">
        <light state="ON" color="GREEN"/>
        <useMacro macro="M1" />
      </case>
      <case op="exact" value="2">
        <light state="ON" color="BLUE"/>
      </case>
      <default>
        <light state="ON" color="RED"/>
        <switch var="5">
          <case op="exact" value="5">
            <goto target="LOOP1" />
          </case>
        </switch>
      </default>
    </switch>
    <light state="OFF" />
    <goto target="LOOP_M" />
  </script>
  <macros>
    <macro id="M1">
      <light id="LOOP_M" state="ON" color="YELLOW"/>
      <light state="ON" color="PINK"/>
    </macro>
  </macros>
</evaml>